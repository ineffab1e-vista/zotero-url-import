{
  "name": "zotero-url-import",
  "type": "module",
  "version": "0.1.0-alpha.1",
  "description": "Expands Zotero's URL scheme to import items via URL parameters and queries.",
  "config": {
    "addonName": "URL Import",
    "addonID": "zotero-url-import@euclpts.com",
    "addonRef": "zotero-url-import",
    "addonInstance": "zoteroURLImport",
    "prefsPrefix": "extensions.zotero.zotero-url-import"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ineffab1e-vista/zotero-url-import.git"
  },
  "author": "ineffab1e-vista",
  "bugs": {
    "url": "https://github.com/ineffab1e-vista/zotero-url-import/issues"
  },
  "homepage": "https://github.com/ineffab1e-vista/zotero-url-import#readme",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "zotero-plugin serve",
    "build": "zotero-plugin build && tsc --noEmit",
    "format": "prettier -c .",
    "lint": "eslint .",
    "release": "zotero-plugin release",
    "test": "zotero-plugin test",
    "update-deps": "npm update --save"
  },
  "dependencies": {
    "zotero-plugin-toolkit": "^5.1.0-beta.6"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.10.0",
    "@zotero-plugin/eslint-config": "^0.6.7",
    "chai": "^5.2.1",
    "eslint": "^9.39.1",
    "mocha": "^11.7.5",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "zotero-plugin-scaffold": "^0.8.1",
    "zotero-types": "^4.1.0-beta.1"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": [
          "*.xhtml"
        ],
        "options": {
          "htmlWhitespaceSensitivity": "css"
        }
      }
    ]
  }
}
